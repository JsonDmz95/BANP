%tr{class: "#{provider.state}"}
  -# Set attributes
  - attributes = ["name", "FEIN", "email", "Phone"]

  %td
    %p= provider.id

  - attributes.each do |attribute|
    %td
      %p= provider["#{attribute}"]

  %td.align-middle
    %p
      = button_tag class: "btn btn-general select-provider", "data-id": provider.id, "data-name": "#{provider.name}", "data-toggle": "tooltip", "data-placement": "bottom", title: t("views.index.select") do
        %i.fas.fa-check
