%tr{class: "clickable-row #{provider.state}", "data-href": "#{admin_provider_path(provider)}"}
  -# Set attributes
  - attributes = ["name", "FEIN", "email" ,"phone"]

  %td
    = link_to admin_provider_path(provider), class: "record-link", "data-header": "No." do
      %p= provider.id

  - attributes.each do |attribute|
    %td
      = link_to admin_provider_path(provider), class: "record-link", "data-header": t("activerecord.attributes.provider.#{attribute}") do
        %p= provider["#{attribute}"]
